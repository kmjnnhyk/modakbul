{
    "name": "modakbul",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "turbo run build",
        "db:migrate:deploy": "turbo run db:migrate:deploy",
        "db:push": "turbo run db:push",
        "db:seed": "turbo run db:seed",
        "dev": "turbo run dev",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "generate": "turbo run generate",
        "lint": "turbo run lint"
    },
    "devDependencies": {
        "turbo": "^2.3.3",
        "prettier": "^3.4.2"
    },
    "engines": {
        "node": ">=18"
    },
    "packageManager": "pnpm@9.9.0"
}
